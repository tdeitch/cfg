#! /usr/bin/env python2

# urldecode
# Convert %xx entities to their single-character equivalents.
# If replace_plus is true, also replaces `+` with space.
replace_plus=True

import sys, urllib
if replace_plus:
    print urllib.unquote_plus(' '.join(sys.argv[1:]))
else:
    print urllib.unquote(' '.join(sys.argv[1:]))
